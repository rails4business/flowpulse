<div id="<%= dom_id slot_instance %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Slot template:</strong>
    <%= slot_instance.slot_template_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Date start:</strong>
    <%= slot_instance.date_start %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Date end:</strong>
    <%= slot_instance.date_end %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= slot_instance.status %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Notes:</strong>
    <%= slot_instance.notes %>
  </div>
</div>
