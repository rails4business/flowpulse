<div id="<%= dom_id contact %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">First name:</strong>
    <%= contact.first_name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Last name:</strong>
    <%= contact.last_name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Lead:</strong>
    <%= contact.lead_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Email:</strong>
    <%= contact.email %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Phone:</strong>
    <%= contact.phone %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Date of birth:</strong>
    <%= contact.date_of_birth %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Place of birth:</strong>
    <%= contact.place_of_birth %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Fiscal code:</strong>
    <%= contact.fiscal_code %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Vat number:</strong>
    <%= contact.vat_number %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Billing name:</strong>
    <%= contact.billing_name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Billing address:</strong>
    <%= contact.billing_address %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Billing zip:</strong>
    <%= contact.billing_zip %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Billing city:</strong>
    <%= contact.billing_city %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Billing country:</strong>
    <%= contact.billing_country %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Meta:</strong>
    <%= contact.meta %>
  </div>
</div>
