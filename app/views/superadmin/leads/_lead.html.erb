
<div id="<%= dom_id lead %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Lead id</strong>
    <%= lead.id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= lead.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Surname:</strong>
    <%= lead.surname %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Username:</strong>
    <%= lead.username %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Email:</strong>
    <%= lead.email %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Phone:</strong>
    <%= lead.phone %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Token:</strong>
    <%= lead.token %>
  </div>
  <div>
    <strong class="block font-medium mb-1">User:</strong>
    <%= lead.user_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Parent:</strong>
    <%= lead.parent_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Referral lead:</strong>
    <%= lead.referral_lead_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Meta:</strong>
    <%= lead.meta %>
  </div>
</div>
