<div id="<%= dom_id post %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">lead:</strong>
    <%= post.lead_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Title:</strong>
    <%= post.title %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Slug:</strong>
    <%= post.slug %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= post.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Thumb url:</strong>
    <%= post.thumb_url %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Cover url:</strong>
    <%= post.cover_url %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Banner url:</strong>
    <%= post.banner_url %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Content:</strong>
    <%= post.content %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Published at:</strong>
    <%= post.taxbranch&.published_at %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Taxbranch:</strong>
    <%= post.taxbranch_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= post.taxbranch&.status %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Meta:</strong>
    <%= post.meta %>
  </div>
</div>
